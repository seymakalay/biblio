@article{ ISI:000382879500004,
Author = {Fu, Xiaolan and Akter, Shaheen},
Title = {{The Impact of Mobile Phone Technology on Agricultural Extension Services
   Delivery: Evidence from India}},
Journal = {{JOURNAL OF DEVELOPMENT STUDIES}},
Year = {{2016}},
Volume = {{52}},
Number = {{11}},
Pages = {{1561-1576}},
Abstract = {{This study examines the impact of mobile phone enhanced intervention in
   agricultural extension service delivery in India. Findings show that the
   amount, quality and speed of service delivery have improved
   significantly because of the intervention. There are also benefits in
   terms of greater knowledge and awareness of new agricultural practices,
   farmers' aspiration to try new technology in the future and access to
   credit. The system does not discriminate against farmers from
   disadvantaged and low education backgrounds. Thus, a system of well-used
   technology, which is assisted by trained village youths, can serve as a
   tool for inclusive development.}},
