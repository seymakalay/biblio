@article{ ISI:000312286200012,
Author = {Shoji, Masahiro and Aoyagi, Keitaro and Kasahara, Ryuji and Sawada,
   Yasuyuki and Ueyama, Mika},
Title = {{Social Capital Formation and Credit Access: Evidence from Sri Lanka}},
Journal = {{WORLD DEVELOPMENT}},
Year = {{2012}},
Volume = {{40}},
Number = {{12}},
Pages = {{2522-2536}},
Month = {{DEC}},
Abstract = {{While previous studies evaluate the impact of social capital on
   development outcomes, there are very few empirical studies on
   determinants of social capital formation. We use unique long panel data
   from Sri Lanka to examine the mechanism of social capital formation in
   an imperfect credit market. We show that households facing credit
   constraints reduce investments in social capital. Furthermore, temporal
   declines in investment persistently reduce general trust, trust in
   villagers, and trust in business partners. While previous studies argue
   that social capital improves access to informal credit, we show the
   reverse causality. Combining these findings suggests a potential poverty
   trap. (C) 2012 Elsevier Ltd. All rights reserved.}},
