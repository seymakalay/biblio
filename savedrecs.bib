@article{ ISI:000076015500019,
Author = {Angelini, P and Di Salvo, R and Ferri, G},
Title = {{Availability and cost of credit for small businesses: Customer
   relationships and credit cooperatives}},
Journal = {{JOURNAL OF BANKING \& FINANCE}},
Year = {{1998}},
Volume = {{22}},
