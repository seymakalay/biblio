@article{ ISI:000180794200011,
Author = {Fisman, R and Love, I},
Title = {{Trade credit, financial intermediary development, and industry growth}},
Journal = {{JOURNAL OF FINANCE}},
Year = {{2003}},
Volume = {{58}},
Number = {{1}},
Pages = {{353-374}},
Month = {{FEB}},
Abstract = {{Recent work suggests that financial development is important for
   economic growth, since financial markets more effectively allocate
   capital to firms with high value projects. For firms in poorly developed
   financial markets, implicit borrowing in the form of trade credit may
   provide an alternative source of funds. We show that industries with
   higher dependence on trade credit financing exhibit higher rates of
   growth in countries with weaker financial institutions. Furthermore,
   consistent with barriers to trade credit access among young firms, we
   show that most of the effect that we report comes from growth in the
   size of preexisting firms.}},
