@article{ ISI:000179577000006,
Author = {Petersen, MA and Rajan, RG},
Title = {{Does distance still matter? The information revolution in small business
   lending}},
Journal = {{JOURNAL OF FINANCE}},
Year = {{2002}},
Volume = {{57}},
Number = {{6}},
Pages = {{2533-2570}},
Month = {{DEC}},
Abstract = {{The distance between small firms and their lenders is increasing, and
   they are communicating in more impersonal ways. After documenting these
   systematic changes, we demonstrate they do not arise from small firms
   locating differently, consolidation in the banking industry, or biases
   in the sample. Instead, improvements in lender productivity appear to
   explain our findings. We also find distant firms no longer have to be
   the highest quality credits, indicating they have greater access to
   credit. The evidence indicates there has been substantial development of
   the financial sector, even in areas such as small business lending.}},
