@article{ ISI:000298026700005,
Author = {Gopalan, Radhakrishnan and Udell, Gregory F. and Yerramilli, Vijay},
Title = {{Why Do Firms Form New Banking Relationships?}},
Journal = {{JOURNAL OF FINANCIAL AND QUANTITATIVE ANALYSIS}},
Year = {{2011}},
Volume = {{46}},
Number = {{5}},
Pages = {{1335-1365}},
Month = {{OCT}},
Abstract = {{Using a large loan sample from 1990 to 2006, we examine why firms form
   new banking relationships. Small public firms that do not have existing
   relationships with large banks are more likely to form new banking
   relationships. On average, firms obtain higher loan amounts when they
   form new banking relationships, while small firms also experience an
   increase in sales growth, capital expenditure, leverage, analyst
   coverage, and public debt issuance subsequently. Our findings suggest
   that firms form new banking relationships to expand their access to
   credit and capital market services, and highlight an important cost of
   exclusive banking relationships.}},
