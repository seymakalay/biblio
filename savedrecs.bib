@article{ ISI:000273705700012,
Author = {Karlan, Dean and Zinman, Jonathan},
Title = {{Expanding Credit Access: Using Randomized Supply Decisions to Estimate
   the Impacts}},
Journal = {{REVIEW OF FINANCIAL STUDIES}},
Year = {{2010}},
Volume = {{23}},
Number = {{1}},
Pages = {{433-464}},
Month = {{JAN}},
Abstract = {{Expanding access to commercial credit is a key ingredient of financial
   development strategies. There is less consensus on whether expanding
   access to consumer credit helps borrowers, particularly when loans are
   extended at high interest rates. Popular skepticism about
   ``unproductive,{''} ``usurious{''} lending is fueled by research
   highlighting behavioral biases that may induce overborrowing. We
   estimate the impacts of expanding access to consumer credit at a 200\%
   annual percentage rate (APR) using a field experiment and follow-up data
   collection. The randomly assigned marginal loans produced significant
   net benefits for borrowers across a wide range of outcomes. There is
   also some evidence that the loans were profitable.}},
