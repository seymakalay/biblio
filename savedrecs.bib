@article{ ISI:000375377000014,
Author = {Walelign, Solomon Zena},
Title = {{Livelihood strategies, environmental dependency and rural poverty: the
   case of two villages in rural Mozambique}},
Journal = {{ENVIRONMENT DEVELOPMENT AND SUSTAINABILITY}},
