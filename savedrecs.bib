@article{ ISI:000352747600001,
Author = {Laepple, Doris and Renwick, Alan and Thorne, Fiona},
Title = {{Measuring and understanding the drivers of agricultural innovation:
   Evidence from Ireland}},
Journal = {{FOOD POLICY}},
Year = {{2015}},
Volume = {{51}},
Pages = {{1-8}},
Month = {{FEB}},
Abstract = {{Despite the well-known importance of innovation to productivity growth
   in the agri-food sector, very few studies have attempted to measure
   farm-level innovation. This article contributes to the literature by
   developing an agricultural innovation index that goes beyond measuring
   innovation through adopted technologies. Based on this index, drivers
   and barriers of innovation are assessed. The findings reveal that
   innovation efforts differ between farm systems. Moreover, farm size and
   intensity, access to credit and agricultural education foster
   innovation, while increasing age and working off-farm hinder innovation.
   The paper concludes with policy recommendations to facilitate innovation
   in the agri-food sector. (C) 2014 Elsevier Ltd. All rights reserved.}},
