@article{ ISI:000316162900017,
Author = {Benbouzid, Nadia and Mallick, Sushanta},
Title = {{Determinants of bank credit default swap spreads: The role of the
   housing sector}},
Journal = {{NORTH AMERICAN JOURNAL OF ECONOMICS AND FINANCE}},
Year = {{2013}},
Volume = {{24}},
Pages = {{243-259}},
Month = {{JAN}},
Abstract = {{This paper relates credit spreads (CDS prices) in the UK banking sector
   with the performance of the housing sector. Using data on banking sector
   CDS spreads for the period January 2004 to April 2011, we find that
   house price dynamics are a key driving factor behind the increase in
   credit spreads as reflected in CDS prices. Also we find that as stock
   prices increase, both bank capital and bank borrowing capacity increase
   that in turn decreases credit risk. Furthermore as banking sector
   liquidity increases banks tend to lend to less credit-worthy (subprime)
   borrowers that in turn increases credit risk in the banking sector.
   Collectively the results shed light on the determinants of credit risk
   in the banking sector. (c) 2012 Elsevier Inc. All rights reserved.}},
